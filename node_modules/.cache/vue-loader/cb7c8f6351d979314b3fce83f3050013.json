     <thead>\n        <tr>\n          <th>Item Name</th>\n          <th>Online Store</th>\n          <th>Price</th>\n          <th>Delivery EST Date</th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr v-for=\"(item, index) in items\" :key=\"index\">\n          <td>{{item.name}}</td>\n          <td>{{item.store}}</td>\n          <td>{{item.price}}</td>\n          <td>{{item.date}}</td>\n          <td><button @click='Received(item)' class='btn btn-primary'>Received</button></td>\n        </tr>\n      </tbody>\n    </table>\n  </div>\n</template>\n\n<script>\nexport default {\n  props: {\n    name: {required: true},\n    selected: {default: false}\n  },\n  data() {\n    return {\n      isActive: false,\n    };\n  },\n  mounted() {\n    this.isActive = this.selected;\n  },\n  computed: {\n    items() {\n      let storeItems = this.$store.getters.itemToItemList;\n\n      return sortedByDateItems(storeItems);\n    },\n  },\n  methods: {\n    sortedByDateItems(items) {\n      let itemsToSort = items;\n      let sortedItems = itemsToSort.sort((a, b) => {\n        let aDate = new Date(a.date).getTime();\n        let bDate = new Date(b.date).getTime();\n        return (aDate >= bDate) ? 1 : -1;\n      })\n      return sortedItems;\n    },\n  },\n}\n</script>\n\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n</style>\n"]}]}