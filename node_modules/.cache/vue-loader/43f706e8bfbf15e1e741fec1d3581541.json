     <tr>\n          <th>Item Name</th>\n          <th>Online Store</th>\n          <th>Price</th>\n          <th>Delivery EST Date</th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr v-for=\"(item, index) in filterdByDateProducts\" :key=\"index\">\n          <td>{{item.name}}</td>\n          <td>{{item.store}}</td>\n          <td>{{item.price}}</td>\n          <td>{{item.date}}</td>\n          <td><button @click='addToCart(item)' class='btn btn-primary'>Add to cart</button></td>\n        </tr>\n      </tbody>\n    </table>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'products',\n  props: {},\n  data() {\n    return {\n      products: [\n        {\n          \"name\": \"iPhone 11\",\n          \"store\": \"amazon\",\n          \"price\": \"23.56 (USD Only)\",\n          \"date\": \"01/12/2020\"\n        },\n        {\n          \"name\": \"iPhone 12\",\n          \"store\": \"ebay\",\n          \"price\": \"29.55 (NIS Only)\",\n          \"date\": \"02/12/2020\"\n        }]\n    };\n  },\n  computed: {\n    filterdByDateProducts() {\n      let productsToFilter = this.products;\n      let filterdProducts = productsToFilter.sort((a, b) => {\n        let aDate = new Date(a.date).getTime();\n        let bDate = new Date(b.date).getTime();\n        return (aDate >= bDate) ? 1 : -1;\n      })\n      return filterdProducts;\n    },\n  }\n}\n</script>\n\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n</style>\n"]}]}