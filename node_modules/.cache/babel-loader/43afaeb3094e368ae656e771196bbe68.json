       \"name\": \"iPhone 12\",\r\n            \"store\": \"ebay\",\r\n            \"price\": \"29.55\",\r\n            \"date\": \"02/12/2020\",\r\n            \"id\": \"2\"\r\n        },\r\n        {\r\n            \"name\": \"iPhone 10\",\r\n            \"store\": \"ebay\",\r\n            \"price\": \"29.55\",\r\n            \"date\": \"02/12/2020\",\r\n            \"id\": \"3\"\r\n        }\r\n    ],\r\n\r\n};\r\nlet mutations = {\r\n    toggleNav() {\r\n        store.isNavOpen = !store.isNavOpen\r\n    },\r\n    received(state, item) {\r\n        console.log(\"item \", item)\r\n\r\n        state.receivedItems.push(item);\r\n    },\r\n};\r\nlet getters = {\r\n    itemToItemList(state) {\r\n        let updatedItems = [];\r\n        if (state.receivedItems.length < 1) {\r\n            return state.items;\r\n        }\r\n        state.items.forEach(item => {\r\n            let isInReceivedItems = false;\r\n            state.receivedItems.forEach(receivedItem => {\r\n                if (item.id == receivedItem.id) {\r\n                    isInReceivedItems = true;\r\n                }\r\n            });\r\n            if (!isInReceivedItems) {\r\n                updatedItems.push(item);\r\n            }\r\n        });\r\n        return updatedItems;\r\n    },\r\n    getStores(state) {\r\n        let storesData = {};\r\n        state.stores.forEach(store => {\r\n            storesData[store] = 0;\r\n        });\r\n        state.receivedItems.forEach(item => {\r\n            storesData[item.store]++;\r\n        });\r\n        return storesData;\r\n    },\r\n    getStoresTotalSum(state) {\r\n        let sum = 0;\r\n        state.receivedItems.forEach(item => {\r\n            sum +=  (0 + item.price);\r\n        });\r\n        return sum;\r\n    },\r\n}\r\n\r\n\r\nexport const store = new Vuex.Store({\r\n    state: state,\r\n    mutations: mutations,\r\n    getters: getters\r\n})"]}]}