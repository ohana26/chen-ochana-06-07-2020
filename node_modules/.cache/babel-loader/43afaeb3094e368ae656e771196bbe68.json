n            let isInReceivedItems = false;\r\n            state.receivedItems.forEach(receivedItem => {\r\n                if (item.id == receivedItem.id) {\r\n                    isInReceivedItems = true;\r\n                }\r\n            });\r\n            if (!isInReceivedItems) {\r\n                updatedItems.push(item);\r\n            }\r\n        });\r\n        return updatedItems;\r\n    },\r\n    getStores() {\r\n        return state.stores;\r\n    },\r\n}\r\n\r\n\r\nexport const store = new Vuex.Store({\r\n    state: state,\r\n    mutations: mutations,\r\n    getters: getters\r\n})"]}]}